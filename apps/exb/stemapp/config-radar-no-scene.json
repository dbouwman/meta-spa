{
  "skin": "radar",
  "attributes": {
    "title": "Radar Experience",
    "portalUrl": "https://cityxcommunity.maps.arcgis.com/sharing/rest"
  },

  "pages": {
    "default": {
      "id": "default",
      "label": "main page",
      "layout": "main-page-layout",
      "mobileLayout": "main-page-layout-mobile",
      "widgets": [
        "arcgis-map"
      ],
      "sections": [
        "sidebar-section"
      ],
      "sharedArea": {
        "header": true
      }
    },
    "detail-page": {
      "id": "detail-page",
      "label": "detail page",
      "layout": "detail-page-layout",
      "mobileLayout": "detail-page-layout-mobile",
      "widgets": [
        "arcgis-map-overview",
        "feature-info-large",
        "feature-info-large2",
        "feature-info3",
        "details-page_related-features",
        "details-page_view-map-link",
        "approved-comments"
      ],
      "sections": [],
      "sharedArea": {
        "header": true
      }
    }
  },

  "sharedArea": {
    "header": {
      "widgets": [
        "header-title-text",
        "user-menu"
      ]
    },
    "footer": {},
    "leftArea": {},
    "rightArea": {},
    "layout": "app-layout"
  },

  "sections": {
    "sidebar-section": {
      "id": "sidebar-section",
      "views": [
        "main-view",
        "list-view",
        "details-view"
      ]
    }
  },

  "views": {
    "main-view": {
      "id": "main-view",
      "label": "main view",
      "layout": "main-view-layout",
      "mobileLayoutCompact": "main-view-compact-layout",
      "mobileTheresholdSize": {
        "height": 300
      },
      "widgets": [
        "main-image",
        "main-text-title",
        "main-text-description",
        "feature-list-main",
        "main-view-places-link"
      ]
    },
    "list-view": {
      "id": "list-view",
      "label": "list view",
      "layout": "list-view-layout",
      "widgets": [
        "list-view-link", "feature-list"
      ]
    },
    "details-view": {
      "id": "details-view",
      "label": "details view",
      "layout": "details-view-layout",
      "mobileLayoutCompact": "details-view-compact-layout",
      "mobileTheresholdSize": {
        "height": 300
      },
      "widgets": [
        "detail-view-link", "feature-info", "feature-info2", "approved-comments", "detail-comment-link", "detail-view-more-link"
      ]
    }
  },

